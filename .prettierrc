{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "importOrder": [
    "<TYPES>",
    "",
    "<BUILTIN_MODULES>",
    "",
    "^react(-dom)?(/.*)?$",
    "",
    "^hono(/.*)?$",
    "",
    "^(?!(?:node:)|[./]|@(?:backend|frontend|shared|tests)/).+",
    "",
    "^@(?:backend|frontend|shared|tests)(?!/.*\\.(?:css|scss|sass|less)$)(/.*)?$",
    "",
    "^(?!.*\\.(css|scss|sass|less)$)[./].*$",
    "",
    "\\.(css|scss|sass|less)$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators"],
  "importOrderSafeSideEffects": ["^.+\\.(css|scss|sass|less)$"],
  "importOrderTypeScriptVersion": "5.0.0",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 130,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
